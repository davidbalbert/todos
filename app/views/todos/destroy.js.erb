$("#todo-<%= @todo.id %>").remove();

$("#list-header").
    replaceWith("<%=j render partial: 'lists/header',
    locals: {list: @list}%>");

<% if @list.todos.count == 0 %>
    $("#todos-container").
        replaceWith("<%=j render partial: 'todos_container',
                    locals: {list: @list}%>");
<% end %>
